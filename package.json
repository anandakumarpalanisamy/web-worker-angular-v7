{
  "name": "web-worker-v7",
  "version": "0.0.0",
  "scripts": {
    "start": "run-p \"npm-watch _build:webworkers:dev\" \"ng run web-worker-v7:serve\" \"ng run web-worker-v7:serve-ie11\"",
    "build": "run-s \"webpack -- --config ./src/web-worker.webpack.production.config.js\" \"webpack -- --config ./src/web-worker.webpack.production.ie11.config.js\" \"ng run web-worker-v7:build:production\" \"ng run web-worker-v7:build-ie11:production\"",
    "build:dev": "run-s _build:webworkers:dev \"ng run web-worker-v7:build\" \"ng run web-worker-v7:build-ie11\"",
    "test": "run-p \"npm-watch _build:webworkers:dev\" \"ng run web-worker-v7:test -- --code-coverage\" \"ng run web-worker-v7:test-ie11\"",
    "lint": "ng lint",
    "e2e": "run-s _build:webworkers:dev \"ng e2e web-worker-v7-e2e\" \"ng e2e web-worker-v7-e2e-ie11\" \"ng e2e web-worker-v7-e2e-edge\"",
    "format": "prettier ./{src,e2e}/**/*.{ts,json,scss} ./*.{json,md} --write",
    "clean": "rimraf ./coverage ./dist ./src/assets/evergreen-webworkers ./src/assets/ie11-webworkers",
    "_build:webworkers:dev": "run-s \"webpack -- --config ./src/web-worker.webpack.dev.config.js\" \"webpack -- --config ./src/web-worker.webpack.dev.ie11.config.js\"",
    "ng": "ng",
    "npm-watch": "npm-watch",
    "webpack": "webpack"
  },
  "watch": {
    "_build:webworkers:dev": {
      "patterns": [
        "src/**/*.worker.ts"
      ],
      "extensions": "ts",
      "quiet": true
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0-beta.7",
    "@angular/common": "^7.0.0-beta.7",
    "@angular/compiler": "^7.0.0-beta.7",
    "@angular/core": "^7.0.0-beta.7",
    "@angular/forms": "^7.0.0-beta.7",
    "@angular/http": "^7.0.0-beta.7",
    "@angular/platform-browser": "^7.0.0-beta.7",
    "@angular/platform-browser-dynamic": "^7.0.0-beta.7",
    "@angular/platform-webworker": "^7.0.0-beta.7",
    "@angular/platform-webworker-dynamic": "^7.0.0-beta.7",
    "@angular/router": "^7.0.0-beta.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.3",
    "vega": "^4.2.0",
    "vega-embed": "^3.20.0",
    "vega-lite": "^3.0.0-rc6",
    "web-animations-js": "^2.3.1",
    "whatwg-fetch": "^3.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^2.4.1",
    "@angular-builders/dev-server": "^2.2.3",
    "@angular-devkit/build-angular": "~0.9.0-beta.1",
    "@angular/cli": "~7.0.0-beta.4",
    "@angular/compiler-cli": "^7.0.0-beta.7",
    "@angular/language-service": "^7.0.0-beta.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.4.0",
    "prettier": "^1.14.3",
    "protractor": "~5.4.0",
    "puppeteer": "^1.8.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.0.1",
    "webpack-cli": "^3.1.1"
  }
}
